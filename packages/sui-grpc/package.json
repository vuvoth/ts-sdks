{
	"name": "@mysten/sui-grpc",
	"version": "0.0.0",
	"description": "A typescript GRPC client for sui",
	"license": "Apache-2.0",
	"author": "Mysten Labs <build@mystenlabs.com>",
	"sideEffects": false,
	"type": "commonjs",
	"main": "./dist/cjs/index.js",
	"module": "./dist/esm/index.js",
	"types": "./dist/cjs/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js"
		}
	},
	"files": [
		"CHANGELOG.md",
		"dist",
		"src"
	],
	"scripts": {
		"clean": "rm -rf tsconfig.tsbuildinfo ./dist",
		"dev": "pnpm build --watch",
		"build": "build-package",
		"prepublishOnly": "pnpm turbo build",
		"prettier:check": "prettier -c --ignore-unknown .",
		"prettier:fix": "prettier -w --ignore-unknown .",
		"eslint:check": "eslint --max-warnings=0 .",
		"eslint:fix": "pnpm run eslint:check --fix",
		"lint": "pnpm run eslint:check && pnpm run prettier:check",
		"lint:fix": "pnpm run eslint:fix && pnpm run prettier:fix",
		"generate:proto": "protoc --ts_out ./src/proto --ts_opt server_none -I ../../../sui/crates/sui-rpc-api/proto -I ../../../sui/crates/sui-rpc-api/proto ../../../sui/crates/sui-rpc-api/proto/sui/rpc/*/*.proto",
		"generate": "rm -rf src/proto && mkdir src/proto && pnpm generate:proto && pnpm lint:fix"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/MystenLabs/ts-sdks.git"
	},
	"keywords": [
		"sui",
		"grpc"
	],
	"bugs": {
		"url": "https://github.com/MystenLabs/ts-sdks/issues"
	},
	"homepage": "https://github.com/MystenLabs/ts-sdks#readme",
	"devDependencies": {
		"@mysten/build-scripts": "workspace:*",
		"@protobuf-ts/plugin": "^2.9.6",
		"typescript": "^5.8.3",
		"vitest": "^3.2.1"
	},
	"dependencies": {
		"@mysten/sui": "workspace:*",
		"@mysten/utils": "workspace:*",
		"@protobuf-ts/grpcweb-transport": "^2.9.6",
		"@protobuf-ts/runtime": "^2.9.6",
		"@protobuf-ts/runtime-rpc": "^2.9.6"
	}
}
