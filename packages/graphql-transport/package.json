{
	"name": "@mysten/graphql-transport",
	"version": "0.2.54",
	"description": "A GraphQL transport to allow SuiClient to work with RPC 2.0",
	"license": "Apache-2.0",
	"author": "Mysten Labs <build@mystenlabs.com>",
	"type": "commonjs",
	"main": "./dist/cjs/index.js",
	"module": "./dist/esm/index.js",
	"types": "./dist/cjs/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/cjs/index.js"
		}
	},
	"sideEffects": false,
	"files": [
		"CHANGELOG.md",
		"dist"
	],
	"scripts": {
		"build": "build-package",
		"clean": "rm -rf tsconfig.tsbuildinfo ./dist",
		"codegen": "graphql-codegen --config codegen.ts",
		"eslint:check": "eslint --max-warnings=0 .",
		"eslint:fix": "pnpm run eslint:check --fix",
		"lint:fix": "pnpm run eslint:fix && pnpm run prettier:fix",
		"lint": "pnpm run eslint:check && pnpm run prettier:check",
		"prepublishOnly": "pnpm turbo build",
		"prettier:check": "prettier -c --ignore-unknown .",
		"prettier:fix": "prettier -w --ignore-unknown .",
		"test:e2e": "vitest run"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/mystenlabs/ts-sdks.git"
	},
	"bugs": {
		"url": "https://github.com/mystenlabs/ts-sdks/issues"
	},
	"homepage": "https://github.com/mystenlabs/ts-sdks#readme",
	"devDependencies": {
		"@graphql-codegen/add": "^5.0.3",
		"@graphql-codegen/cli": "^5.0.5",
		"@graphql-codegen/typed-document-node": "^5.1.1",
		"@graphql-codegen/typescript": "4.1.6",
		"@graphql-codegen/typescript-document-nodes": "4.0.16",
		"@graphql-codegen/typescript-operations": "^4.6.0",
		"@mysten/build-scripts": "workspace:*",
		"@parcel/watcher": "^2.5.1",
		"@types/node": "^22.13.17",
		"graphql-config": "^5.0.3",
		"typescript": "^5.8.2",
		"vitest": "^3.1.1",
		"wait-on": "^8.0.3"
	},
	"dependencies": {
		"@graphql-typed-document-node/core": "^3.2.0",
		"@mysten/bcs": "workspace:*",
		"@mysten/sui": "workspace:*",
		"graphql": "^16.9.0"
	}
}
