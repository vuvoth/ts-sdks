{
	"name": "@mysten/codegen",
	"version": "0.8.0",
	"description": "typescript codegen for sui move",
	"license": "Apache-2.0",
	"author": "Mysten Labs <build@mystenlabs.com>",
	"sideEffects": false,
	"type": "module",
	"main": "./dist/index.mjs",
	"types": "./dist/index.d.mts",
	"exports": {
		".": {
			"types": "./dist/index.d.mts",
			"import": "./dist/index.mjs"
		}
	},
	"files": [
		"CHANGELOG.md",
		"dist",
		"src"
	],
	"bin": {
		"sui-ts-codegen": "dist/bin/cli.mjs",
		"__sui-ts-codegen_bash_complete": "dist/bin/bash-complete.mjs"
	},
	"scripts": {
		"clean": "rm -rf tsconfig.tsbuildinfo ./dist",
		"dev": "pnpm build --watch",
		"build": "rm -rf dist && tsc --noEmit && tsdown",
		"codegen": "pnpm tsx src/bin/cli.ts generate && pnpm lint:fix",
		"test": "vitest run",
		"test:watch": "vitest",
		"prettier:check": "prettier -c --ignore-unknown .",
		"prettier:fix": "prettier -w --ignore-unknown .",
		"oxlint:check": "oxlint  .",
		"oxlint:fix": "oxlint --fix",
		"lint": "pnpm run oxlint:check && pnpm run prettier:check",
		"lint:fix": "pnpm run oxlint:fix && pnpm run prettier:fix"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/MystenLabs/ts-sdks.git"
	},
	"keywords": [
		"move",
		"codegen",
		"sui"
	],
	"bugs": {
		"url": "https://github.com/mystenlabs/ts-sdks/issues"
	},
	"homepage": "https://github.com/MystenLabs/ts-sdks/blob/main/packages/codegen#readme",
	"devDependencies": {
		"vitest": "^4.0.17"
	},
	"dependencies": {
		"@mysten/bcs": "workspace:^",
		"@mysten/sui": "workspace:^",
		"@stricli/auto-complete": "^1.2.5",
		"@stricli/core": "^1.2.5",
		"@types/node": "^25.0.8",
		"cosmiconfig": "^9.0.0",
		"prettier": "^3.7.4",
		"toml": "^3.0.0",
		"typescript": "^5.9.3",
		"zod": "^4.3.5"
	}
}
