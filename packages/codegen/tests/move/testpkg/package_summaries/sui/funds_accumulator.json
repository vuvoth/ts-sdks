  "structs": {
    "Withdrawal": {
      "index": 0,
      "doc": " Allows for withdrawing funds from a given address. The `Withdrawal` can be created in PTBs for\n the transaction sender, or dynamically from an object via `withdraw_from_object`.\n The redemption of the funds must be initiated from the module that defines `T`.",
      "attributes": [],
      "abilities": [
        "Drop"
      ],
      "type_parameters": [
        {
          "phantom": true,
          "name": "T",
          "constraints": [
            "Store"
          ]
        }
      ],
      "fields": {
        "positional_fields": false,
        "fields": {
          "owner": {
            "index": 0,
            "doc": " The owner of the funds, either an object or a transaction sender",
            "type_": "address"
          },
          "limit": {
            "index": 1,
            "doc": " At signing we check the limit <= balance when taking this as a call arg.\n If this was generated from an object, we cannot check this until redemption.",
            "type_": "u256"
          }
        }
      }
    }
  },
