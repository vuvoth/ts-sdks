{
	"name": "@mysten/mvr-static",
	"author": "Mysten Labs <build@mystenlabs.com>",
	"description": "The static generation tool for Move Registry (mvr)",
	"homepage": "https://sdk.mystenlabs.com",
	"version": "0.3.0",
	"license": "Apache-2.0",
	"files": [
		"CHANGELOG.md",
		"LICENSE",
		"README.md",
		"dist",
		"src"
	],
	"type": "module",
	"main": "./dist/index.mjs",
	"types": "./dist/index.d.mts",
	"exports": {
		".": {
			"types": "./dist/index.d.mts",
			"import": "./dist/index.mjs"
		}
	},
	"scripts": {
		"clean": "rm -rf tsconfig.tsbuildinfo ./dist",
		"build": "rm -rf dist && tsc --noEmit && tsdown",
		"prettier:check": "prettier -c --ignore-unknown .",
		"prettier:fix": "prettier -w --ignore-unknown .",
		"oxlint:check": "oxlint  .",
		"oxlint:fix": "oxlint --fix",
		"lint": "pnpm run oxlint:check && pnpm run prettier:check",
		"lint:fix": "pnpm run oxlint:fix && pnpm run prettier:fix",
		"test": "vitest run"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/MystenLabs/ts-sdks.git"
	},
	"bugs": {
		"url": "https://github.com/MystenLabs/mvr/issues/new"
	},
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"typescript": "^5.9.3",
		"vitest": "^4.0.17"
	},
	"dependencies": {
		"@mysten/sui": "workspace:^",
		"@types/node": "^25.0.8",
		"enquirer": "^2.4.1",
		"glob": "^13.0.0",
		"prettier": "^3.7.4"
	},
	"sideEffects": false,
	"bin": "./bin/parser.js"
}
